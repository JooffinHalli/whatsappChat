{
  "name": "whatsapp-chat",
  "version": "0.0.1",
  "homepage": "https://jooffinhalli.github.io/whatsappChat/",
  "scripts": {
    "prebuild": "npm i",
    "build": "webpack",
    "watch": "webpack --config webpack.dev.js --watch",
    "server": "node devServer.js",
    "start": "npm-run-all --parallel watch server",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d public",
    "postdeploy": "echo \"check https://jooffinhalli.github.io/whatsappChat/\"",
    "api": "cd npm-scripts && npm run api https://github.com/JooffinHalli/whatsappChat.git main"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.3",
    "gh-pages": "^5.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
